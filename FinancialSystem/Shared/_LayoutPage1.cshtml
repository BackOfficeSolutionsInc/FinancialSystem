
<!DOCTYPE html>
<html>

<head>
	<!--Import Google Icon Font-->
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
	<link href="https://fonts.googleapis.com/css?family=Titillium+Web:400,600,600i,700,700i,900" rel="stylesheet">
	<!-- Compiled and minified CSS -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
	<!--Import materialize.css-->

	<!--Let browser know website is optimized for mobile-->
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	@System.Web.Optimization.Styles.Render("~/Content/PR/pr-shop")
</head>

<body>
	<!-- Navbar goes here -->
	<nav>
		<div class="nav-wrapper">
			<a id="logo-container" href="#" class="brand-logo"><img src="images/bsi_logo.png" alt="" class="logo"></a>
			<a href="#" class="notification"><i class="material-icons white-text">notifications</i> </a><small class="notification-badge yellow accent-2">5</small>
			<a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
			<ul class="right hide-on-med-and-down">
				<li>
				<li>
					<a href="#"><img src="images/profilepic_sample.png" alt="" class="circle responsive-img"></a>
				</li>
				</li>
				<li>
					<a href="#" class="shopping">
						<i class="material-icons white-text">shopping_basket</i><small class="notification-badge yellow accent-2">5</small>
					</a>
				</li>
			</ul>
		</div>
	</nav>

	<ul class="sidenav" id="mobile-demo">
		<li class="active"><a href="#"><i class="material-icons">dashboard</i> Dash Board</a></li>
		<li><a href="#"><i class="material-icons">assignment</i> Purchase Request</a></li>
		<li><a href="#"><i class="material-icons">format_quote</i> Quote Analysis</a></li>
		<li><a href="#"><i class="material-icons">shopping_cart</i> Purchase Order</a></li>
		<li><a href="#"><i class="material-icons">list</i>Items</a></li>
		<li><a href="usermaintenance.html"><i class="material-icons">settings</i> Settings</a></li>
	</ul>
	<!-- Page Layout here -->
	<header>
		<div class="row">
			<div class="col s2">
				<ul id="nav-mobile" class="sidenav sidenav-fixed" style="transform: translateX(-105%);">
					<div class="brand-logo"><img src="images/bsi_logo.png" alt="" class="logo"></div>
					<ul class="collapsible">
						<li>
							<div class="collapsible-header">
								<a href="#">
									<i class="material-icons">dashboard</i> Dash
									Board
								</a>
							</div>
						</li>
						<li>
							<div class="collapsible-header">
								<a href="pr-shop.html">
									<i class="material-icons">assignment</i>
									Purchase Request
								</a>
							</div>
						</li>
						<li>
							<div class="collapsible-header">
								<a href="#">
									<i class="material-icons">format_quote</i> Quote
									Analysis
								</a>
							</div>
						</li>
						<li>
							<div class="collapsible-header">
								<a href="#">
									<i class="material-icons">shopping_cart</i>
									Purchase Order
								</a>
							</div>
						</li>
						<li>
							<div class="collapsible-header">
								<a href="#">
									<i class="material-icons">list</i>
									Items
								</a>
							</div>
						</li>
						<li>
							<div class="collapsible-header">
								<a href="#">
									<i class="material-icons">settings</i>
									Setting
								</a>
							</div>
							<div class="collapsible-body submenu">
								<i class="material-icons icons">fiber_manual_record</i>
								<a href="usermaintenance.html">User Maintenance</a>
							</div>
							<div class="collapsible-body submenu">
								<i class="material-icons icons">fiber_manual_record</i><a href="jobmaintenance.html">Job Maintenance</a>
							</div>
						</li>
					</ul>
				</ul>
			</div>
			<div>
				@RenderBody()
			</div>
		</div>
	</header>

	<script>
        document.addEventListener('DOMContentLoaded', function () {
            var elems = document.querySelectorAll('.sidenav');
            var instances = M.Sidenav.init(elems);
            var elems = document.querySelectorAll('select');
            var instances = M.FormSelect.init(elems);
            var elems = document.querySelectorAll('.dropdown-trigger');
            var instances = M.Dropdown.init(elems);
            var elems = document.querySelectorAll('.collapsible');
            var instances = M.Collapsible.init(elems);
        });
	</script>
	<!-- Compiled and minified JavaScript -->
	<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</body>

</html>